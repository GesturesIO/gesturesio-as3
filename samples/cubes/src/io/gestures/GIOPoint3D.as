package io.gestures 
{
	public class GIOPoint3D 
	{
		public var x:Number = -1;
		public var y:Number = -1;
		public var z:Number = -1;
		public function GIOPoint3D(x:Number = 0.0,y:Number = 0.0,z:Number = 0.0) 
		{
			this.x = x;
			this.y = y;
			this.z = z;
		}
	}
}